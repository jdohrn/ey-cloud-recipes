check process <%= @worker_name %>
  with pidfile /data/<%= @app_name %>/current/tmp/pids/<%= @worker_name %>.pid
  start program = "/data/<%= @app_name %>/current/script/delayed_job -e production start" with timeout 120 seconds
  stop program = "/data/<%= @app_name %>/current/script/delayed_job -e production stop" with timeout 120 seconds
  #if totalmem is greater than 300 MB then restart # eating up memory?
  group dj_<%= @app_name %>
